{
    "name": "clarkeash/turbine",
    "description": "A livewire flux starter kit",
    "type": "library",
    "keywords": ["laravel", "auth", "livewire", "flux"],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/filesystem": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/validation": "^11.0|^12.0",
        "symfony/console": "^7.0",
        "livewire/flux": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Clarkeash\\Turbine\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ashley Clarke",
            "email": "me@ashleyclarke.me"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Clarkeash\\Turbine\\Providers\\TurbineServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^9.0"
    }
}
